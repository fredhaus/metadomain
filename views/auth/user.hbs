<div class="landingResults">
  <div class="headerResult">
    <h5 class="txtwhite">Your next domain search:</h5>
  </div>
  <form class="userSearchInput userSearchInputNext" action="/result">
    <div class="form-group">
      <div class="search">
        <div class="input-group mb-3">
          <div class="input-group-prepend">
            <span class="input-group-text" id="basic-addon1">www.</span>
          </div>
          <input type="text" class="form-control" name="domainSearch" value="{{domainName}}"
            aria-label="domainSearchQuery" aria-describedby="basic-addon1">
        </div>
      </div>
    </div>
    <button type="submit">New Search</button>
  </form>
</div>
<div class="suppliers">
  <div class="logo">
    <img src="/img/Gandi.png">
  </div>
  <div class="logo">
    <img src="/img/namesilo.png">
  </div>
  <div class="logo">
    <img src="/img/namecom.png">
  </div>
</div>
<h1 id="sectionHeader">Your historic search results:</h1>
<hr class="divider">
{{#each searchesObj}}
<div class="allSearchesResult">
  <div class="card">
    <h5 class="card-header allSearchesHeader"><strong class="txtwhite">{{this.domain}}</strong></h5>
    <div class="card">
      <div class="card-body">
        <div class="dealer">
          <p class="card-text">Best Dealer:</p>
          <h6 class="card-text">{{this.host}}</h6>
        </div>
        <div class="price">
          <p class="card-text">Best Price:</p>
          <h6 class="card-text">{{this.price}} â‚¬</h6>
        </div>
        <div class="date">
          <p class="card-text">Search Date:</p>
          <h6 class="card-text">{{this.SearchDate}} {{this.SearchTime}}</h6>
        </div>
        <div class="link">
          <a href="/result?domainSearch={{this.domain}}" class="btn btn-primary">Check Offer Again!</a>
        </div>
      </div>
    </div>
  </div>
</div>
{{/each}}