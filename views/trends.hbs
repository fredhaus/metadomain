                        <h6 class="card-text">{{this.name}}</h6>
                    </div>
                    <div class="price">
                        <p class="card-text">Best Price:</p>
                        <h6 class="card-text">{{this.price}} â‚¬</h6>
                    </div>
                    <div class="link">
                        <a href={{this.redirectURL}} class="btn btn-primary" target="_blank">Check offer!</a>
                    </div>
                </div>
            </div>
        </div>
    </div>
    {{/each}}

